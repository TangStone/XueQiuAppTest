{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "errcode",
    "errmsg",
    "userid",
    "name",
    "department",
    "position",
    "mobile",
    "gender",
    "email",
    "avatar",
    "status",
    "isleader",
    "extattr",
    "telephone",
    "enable",
    "hide_mobile",
    "order",
    "qr_code",
    "alias",
    "is_leader_in_dept",
    "thumb_avatar"
  ],
  "properties": {
    "errcode": {
      "$id": "#/properties/errcode",
      "type": "integer",
      "title": "The Errcode Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "errmsg": {
      "$id": "#/properties/errmsg",
      "type": "string",
      "title": "The Errmsg Schema",
      "default": "",
      "examples": [
        "ok"
      ],
      "pattern": "^(.*)$"
    },
    "userid": {
      "$id": "#/properties/userid",
      "type": "string",
      "title": "The Userid Schema",
      "default": "",
      "examples": [
        "TangLei"
      ],
      "pattern": "TangLei"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "唐磊"
      ],
      "pattern": "^(.*)$"
    },
    "department": {
      "$id": "#/properties/department",
      "type": "array",
      "title": "The Department Schema",
      "items": {
        "$id": "#/properties/department/items",
        "type": "integer",
        "title": "The Items Schema",
        "default": 0,
        "examples": [
          1
        ]
      }
    },
    "position": {
      "$id": "#/properties/position",
      "type": "string",
      "title": "The Position Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "mobile": {
      "$id": "#/properties/mobile",
      "type": "string",
      "title": "The Mobile Schema",
      "default": "",
      "examples": [
        "18845599643"
      ],
      "pattern": "^(.*)$"
    },
    "gender": {
      "$id": "#/properties/gender",
      "type": "string",
      "title": "The Gender Schema",
      "default": "",
      "examples": [
        "1"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "avatar": {
      "$id": "#/properties/avatar",
      "type": "string",
      "title": "The Avatar Schema",
      "default": "",
      "examples": [
        "https://wework.qpic.cn/bizmail/xZftDPibicwdmJ8J8Y3pIVkLeLyNGwVG9b454UA7URFk9f1VMjUQPuVw/0"
      ],
      "pattern": "^(.*)$"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "integer",
      "title": "The Status Schema",
      "default": 0,
      "examples": [
        4
      ]
    },
    "isleader": {
      "$id": "#/properties/isleader",
      "type": "integer",
      "title": "The Isleader Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "extattr": {
      "$id": "#/properties/extattr",
      "type": "object",
      "title": "The Extattr Schema",
      "required": [
        "attrs"
      ],
      "properties": {
        "attrs": {
          "$id": "#/properties/extattr/properties/attrs",
          "type": "array",
          "title": "The Attrs Schema"
        }
      }
    },
    "telephone": {
      "$id": "#/properties/telephone",
      "type": "string",
      "title": "The Telephone Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "enable": {
      "$id": "#/properties/enable",
      "type": "integer",
      "title": "The Enable Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "hide_mobile": {
      "$id": "#/properties/hide_mobile",
      "type": "integer",
      "title": "The Hide_mobile Schema",
      "default": 0,
      "examples": [
        0
      ]
    },
    "order": {
      "$id": "#/properties/order",
      "type": "array",
      "title": "The Order Schema",
      "items": {
        "$id": "#/properties/order/items",
        "type": "integer",
        "title": "The Items Schema",
        "default": 0,
        "examples": [
          8192
        ]
      }
    },
    "qr_code": {
      "$id": "#/properties/qr_code",
      "type": "string",
      "title": "The Qr_code Schema",
      "default": "",
      "examples": [
        "https://open.work.weixin.qq.com/wwopen/userQRCode?vcode=vc625374d76d52eda0"
      ],
      "pattern": "^(.*)$"
    },
    "alias": {
      "$id": "#/properties/alias",
      "type": "string",
      "title": "The Alias Schema",
      "default": "",
      "examples": [
        ""
      ],
      "pattern": "^(.*)$"
    },
    "is_leader_in_dept": {
      "$id": "#/properties/is_leader_in_dept",
      "type": "array",
      "title": "The Is_leader_in_dept Schema",
      "items": {
        "$id": "#/properties/is_leader_in_dept/items",
        "type": "integer",
        "title": "The Items Schema",
        "default": 0,
        "examples": [
          0
        ]
      }
    },
    "thumb_avatar": {
      "$id": "#/properties/thumb_avatar",
      "type": "string",
      "title": "The Thumb_avatar Schema",
      "default": "",
      "examples": [
        "https://wework.qpic.cn/bizmail/xZftDPibicwdmJ8J8Y3pIVkLeLyNGwVG9b454UA7URFk9f1VMjUQPuVw/100"
      ],
      "pattern": "^(.*)$"
    }
  }
}